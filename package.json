{
  "name": "razib-telecom",
  "version": "1.0.0",
  "description": "Razib Telecom ",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start-server": "nodemon server.js --ignore './client/'",
    "start-client": "npm start --prefix client",
    "clientInstall": "npm install --prefix client",
    "build": "npm run build --prefix client",
    "dev": "concurrently \"npm run start-server\" \"npm run start-client\""
  },
  "author": "Rashed",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  }
}
